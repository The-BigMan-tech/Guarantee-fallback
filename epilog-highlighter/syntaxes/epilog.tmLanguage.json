{
    "scopeName":"source.epilog",
    "patterns":[
        {
            "name": "constant.name.el",
            "match": "\\:[a-zA-Z]+" 
        },
        {
            "name": "constant.name.strict.el",
            "match": "\\![a-zA-Z]+" 
        },
        {
            "name": "constant.number.el",
            "match": "-?[0-9]+(\\.[0-9]+)?"
        },
        {
            "name": "constant.alias.el",
            "match": "#[a-zA-Z]+" 
        },
        {
            "name": "constant.predicate.el",
            "match": "\\*[a-zA-Z]+"
        },
        {
            "name": "keyword.alias.el",
            "match": "\\balias\\b" 
        },
        {
            "name": "reference.el",
            "match": "[\\bHe\\b]" 
        },
        {
            "name": "constant.plainword.el",
            "match": "\\b[a-zA-Z]+\\b" 
        },
        {
            "name": "terminator.el",
            "match": "\\."
        },
        {
            "name": "comment.line.el",
            "begin": "//",
            "end": "$" 
        },
        {
            "name": "punctuation.el",
            "match": "[,()\\[\\];?!'\\=]" 
        },
        {
            "name": "punctuation.colon.angle.el",
            "match": "[:<>]" 
        },
        {
            "name": "punctuation.bracket.el",
            "match": "[\\[\\]]" 
        }
    ]
}