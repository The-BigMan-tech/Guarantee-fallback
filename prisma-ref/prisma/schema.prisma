datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
}
generator dbml {
  provider = "prisma-dbml-generator"
}
model User {
    primaryKey Int @id @default(autoincrement())
    name  String 
    email String @unique 
    details account[]
}
model account {
    primaryKey Int @id @default(autoincrement())
    author User @relation(fields: [foreignKey],references: [primaryKey])
    password String
    foreignKey Int @unique @default(autoincrement())
}
model RequestLogs {
    primarykey Int @id @default(autoincrement())
    timestamp DateTime
    method String
    url String
}
model Sample {
    primarykey Int @id @default(autoincrement())
    username String @unique
    userpassword String
}