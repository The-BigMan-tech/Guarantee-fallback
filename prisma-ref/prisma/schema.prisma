datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
}
generator dbml {
  provider = "prisma-dbml-generator"
}
model RequestLogs {
    primarykey Int @id @default(autoincrement())
    requestTimestamp DateTime
    method String
    url String
    responseRecord ResponseLogs?
}
model ResponseLogs {
    primarykey Int @id @default(autoincrement())
    responseTimestamp DateTime
    method String
    url String
    statusCode Int
    responseTime Int
    requestRecord RequestLogs @relation(fields: [foreignkey],references: [primarykey])
    foreignkey Int @unique @default(autoincrement())
}
model Sample {
    primarykey Int @id @default(autoincrement())
    username String @unique
    userpassword String
}